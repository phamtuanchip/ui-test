<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="social-sniff-homepage" enabled="true" preserve-order="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		 <classes>
            <class name="org.exoplatform.selenium.platform.social.sniff.Social_HomePage_Activity"/>
            <class name="org.exoplatform.selenium.platform.social.sniff.Social_Dashboard"/>
            <class name="org.exoplatform.selenium.platform.social.sniff.Social_People"/>
            <class name="org.exoplatform.selenium.platform.social.sniff.Social_HomePage_ActivityComposer"/>
            <class name="org.exoplatform.selenium.platform.social.sniff.Social_HomePage_ActivityStream"/>
        </classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>